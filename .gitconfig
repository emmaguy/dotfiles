[user]
	email = emmag87@gmail.com
	name = Emma Guy
[alias]
	st = status -sb 
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow bold
	remote = green bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow bold
	changed = red bold
	untracked = cyan bold
[push]
	default = current
[diff]
    tool = p4merge
[difftool]
	prompt = false
[difftool "p4merge"]
    cmd = "p4merge.exe $LOCAL $REMOTE"
[merge]
    tool = p4merge
[mergetool]
	prompt = false
[mergetool "p4merge"]
    cmd = "p4merge.exe $BASE $LOCAL $REMOTE $MERGED"
    trustExitCode = true
    keepBackup = false